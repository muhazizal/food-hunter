{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-console": 0,
		"indent": 0,
		"no-tabs": 0,
		"linebreak-style": 0,
		"no-underscore-dangle": 0,
		"no-restricted-globals": 0,
		"object-curly-newline": 0,
		"import/no-extraneous-dependencies": 0,
		"max-len": ["error", { "code": 120 }],
		"no-param-reassign": 0,
		"no-useless-return": 0,
		"no-alert": 0,
		"operator-linebreak": 0,
		"no-undef": 0,
		"no-new": 0,
		"consistent-return": 0,
		"no-prototype-builtins": 0,
		"no-return-assign": 0,
		"no-useless-constructor": 0
	}
}
